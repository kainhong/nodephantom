<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/css/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>

    <form accept-charset="UTF-8" id="invcn-form" method="post" action="/render" enctype="multipart/form-data" class="website-form" autocomplete="off" >

    	<div><p>  <%= title %> </p></div>

        <ul>
            <% sitedata.forEach(function(site){ %>
            <li>SiteUrl : <%= site.SiteUrl %></li>
            <% }) %>
        </ul>

        <ul>
            <% for (var i=0; i < sitedata.length; i++) { %>
            <li><%= sitedata[i] %></li>
            <% } %>
        </ul>

    </form>
  </body>
</html>